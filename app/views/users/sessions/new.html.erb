<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label textfield-demo">
    <%= f.email_field :email, autofocus: true, class: "mdl-textfield__input" %>
    <%= f.label :email, class: "mdl-textfield__label" %>
  </div>

  <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label textfield-demo">
    <%= f.password_field :password, autocomplete: false, class: "mdl-textfield__input" %>
    <%= f.label :password, class: "mdl-textfield__label" %>
  </div>

  
<br />
<br />
  <div class="actions">
    <%= f.submit "Log in", class: "mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent" %>
  </div>
<% end %>
<br />

<%= render "devise/shared/links" %>

<%= debug session[:previous_url] %>
